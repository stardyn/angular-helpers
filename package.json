{
  "name": "@stardyn/angular-helpers",
  "version": "2.0.9",
  "description": "Angular Helper Utilities Package - Collection of useful pipes, directives, validators and utility classes for Angular applications",
  "main": "core.js",
  "module": "core.js",
  "browser": "core.js",
  "umd": "core.js",
  "types": "main.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pwsh -File ./build.ps1",
    "build:prod": "pwsh -File ./build.ps1 -Production",
    "build:npmjs": "pwsh -File ./build_publish_npmjs.ps1",
    "build:private": "pwsh -File ./build_publish.ps1",
    "publish:npmjs": "pwsh -File ./build_publish_npmjs.ps1 -Environment live",
    "publish:private": "pwsh -File ./build_publish.ps1 -Environment live",
    "publish:dry": "pwsh -File ./build_publish.ps1 -DryRun",
    "publish:skip": "pwsh -File ./build_publish.ps1 -SkipBuild",
    "clean": "rimraf dist",
    "dev": "pwsh -File ./build.ps1 -Environment dev",
    "test": "echo 'No tests specified'",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "~5.5.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.4.0",
    "terser-webpack-plugin": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@types/lodash": "^4.17.13",
    "@types/numeral": "^2.0.5",
    "@types/crypto-js": "^4.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "ts-md5": "^1.3.1",
    "numeral": "^2.0.6",
    "crypto-js": "^4.2.0"
  },
  "peerDependencies": {
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0",
    "@angular/forms": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "angular",
    "helpers",
    "utilities",
    "pipes",
    "directives",
    "validators",
    "formatters",
    "typescript",
    "stardyn"
  ],
  "author": "Stardyn Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardyn/angular-helpers.git"
  },
  "homepage": "https://github.com/stardyn/angular-helpers",
  "bugs": {
    "url": "https://github.com/stardyn/angular-helpers/issues"
  }
}